{
  "ApiOptions": {
    "Name": "Toltek.Bbb.ApiV3",
    "Version": "v0.0.1",
    "Description": "Toltek Blue ApiV3",
    "TermsOfService": "https://dev.toltekcloud.com/services/toltek.bbb.apiv3/terms",
    "Scopes": [
      "toltek.blue",
      "openid",
      "profile"
    ]
  },
  "Serilog": {
    "Properties": {
      "ClientId": "Toltek.Bbb.ApiV3",
      "Application": "Toltek.Bbb.ApiV3"
    },
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft.AspNetCore.Hosting": "Warning",
        "Microsoft.AspNetCore.Hosting.Diagnostics": "Warning",
        "Microsoft.AspNetCore.Authentication": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj} <s:{SourceContext}>{NewLine}{Exception}"
        }
      }
    ]
  },
  "OpenTelemetry": {
    "ServiceName": "Toltek.Bbb.ApiV3",
    "ServiceVersion": "v0.0.1"
  },
  "Swashbuckle": {
    "UseXmlComments": false,
    "XmlFilePath": "Toltek.Bbb.ApiV3.xml"
  }
}
